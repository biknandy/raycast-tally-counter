{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tally-counter",
  "title": "Tally Counter",
  "description": "Keep track with a counter",
  "icon": "command-icon.png",
  "author": "bik",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "simple",
      "title": "Simple Counter",
      "description": "Add or subtract to a simple counter",
      "mode": "view"
    },
    {
      "name": "multicounter",
      "title": "Multiple Counters",
      "description": "Keep track of multiple counters at once",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
